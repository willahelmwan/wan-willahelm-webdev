(function(){
    angular
        .module("<%= name %>")
        .controller("<%= entityName %>ListController", <%= entityName %>ListController);

    function <%= entityName %>ListController(<%= entityName %>Service) {
        var model = this;

        function init() {
            findAll();
        }
        init();

        function findAll() {
            <%= entityName %>Service.findAll()
                .then(function(data){
                    model.<%= entityName %>s = data;
                });
        }
    }
})();